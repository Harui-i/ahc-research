{
  "name": "CPython 3.13 Dev Container",
  "build": {
    "dockerfile": ".devcontainer/Dockerfile",
    "context": ".",
    "args": {
      "INSTALL_TOML": "082-3-13_cpython.toml",
      "USERNAME": "vscode"
    }
  },
  "image": "ahc-cpython-dev:3.13",
  "remoteUser": "vscode",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-toolsai.jupyter"
      ],
      "settings": {
        "python.defaultInterpreterPath": "/usr/local/bin/python3.13",
        "python.terminal.activateEnvironment": false
      }
    }
  },
  "postCreateCommand": "python3.13 --version"
}
